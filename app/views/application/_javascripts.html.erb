<script src="https://apis.google.com/js/platform.js" async defer></script>
<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
<script src="https://maps.googleapis.com/maps/api/js?libraries=places&key=<%= ENV.fetch('GOOGLE_KEY') %>" defer data-turbolinks-eval="false"></script>

<% if Rails.env.test? %>
  <%= javascript_tag do %>
    $.fx.off = true;
    $.ajaxSetup({ async: false });
  <% end %>
<% end %>
