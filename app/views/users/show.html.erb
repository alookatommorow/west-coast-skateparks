<div class="container">
  <h1><%= @user.username %>'s page</h1>
  <h2>Favorites<h2>
    <ul>
      <% @myfavorites.each do |fav|%>
        <%if fav.name%>
          <li><%= link_to fav.city + ": " + fav.name, skatepark_path(fav) %></li>
        <% else %>
          <li><%= link_to fav.city, skatepark_path(fav) %></li>
        <% end %>
      <% end %>
    </ul>
</div>
